{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@app/*": ["src/app/*"],
      "@utils/*": ["src/utils/*"],
      "@src/*": ["src/*"],
      "@redux/*": ["src/redux/*"],
      "@services/*": ["src/services/*"],
      "@hooks/*": ["src/hooks/*"],
      "@context/*": ["src/context/*"],
      "@styles/*": ["src/styles/*"],
      "@config/*": ["config/*"],
      "@providers/*": ["src/providers/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": ["**/*.ts", "**/*.tsx"]
}
