{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "moduleResolution": "bundler",
    "module": "es2022",
    "paths": {
      "assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@app/*": ["src/app/*"],
      "@utils/*": ["src/utils/*"],
      "@src/*": ["src/*"],
      "@services/*": ["src/services/*"],
      "@hooks/*": ["src/hooks/*"],
      "@context/*": ["src/context/*"],
      "@styles/*": ["src/styles/*"],
      "@config/*": ["src/config/*"],
      "@providers/*": ["src/providers/*"],
      "@stores/*": ["src/stores/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": ["**/*.ts", "**/*.tsx"]
}
